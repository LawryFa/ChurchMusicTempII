{
  "expo": {
    "name": "Church Music App",
    "slug": "church-music-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "churchmusicapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android"],
    "sdkVersion": "52.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rainbow.churchmusicapp"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1411c171-e3b9-4ad2-b6e1-9a7b54eb2eac"
      }
    },
    "android": {
      "package": "com.rainbow.app"
    }
  }
}
